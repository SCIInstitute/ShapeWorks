    \begin{algocf*}[#1]%
    }
  \@algocfbegin%
  \ifthenelse{\boolean{algocf@slide}}{\parskip 0.5ex\color{black}}{}%    
  }{% begin
  \@algocfend%
  \ifthenelse{\equal{\algocf@float}{figure}}{%
    \end{figure*}%
    }{
    \end{algocf*}%
    }
  \algocf@resetcaption}       % end
