\newcommand{\V@sline}[1]{\al@push{\skiprule}\hbox{\vrule%
  \vtop{\al@push{\skiptext}\vtop{\al@addskiptotal\advance\hsize by -\skiplength #1\vskip 1ex}}}%
  \al@pop{\skiprule}\al@subskiptotal\vskip 1pt\nointerlineskip}
