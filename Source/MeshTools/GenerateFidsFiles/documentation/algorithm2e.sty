\newcommand{\@restorealgo}{%
  \setlength{\hsize}{\algowidth}%
  \lineskip\normallineskip\setlength{\skiptotal}{\@defaultskiptotal}%
  \let\caption=\algocf@oldcaption%
  \let\;=\@mathsemicolon}%% end of \@restorealgo
