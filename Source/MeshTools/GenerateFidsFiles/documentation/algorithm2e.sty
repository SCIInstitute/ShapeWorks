\newcommand{\V@line}[1]{\al@push{\skiprule}\hbox{\vrule%
  \vtop{\al@push{\skiptext}\vtop{\al@addskiptotal\advance\hsize by -\skiplength #1}\Hlne}}%
  \al@pop{\skiprule}\al@subskiptotal\vskip\vespace}
